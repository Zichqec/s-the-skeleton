OnMinuteChange
{
	Hourcheck
	timeslot = GetTimeSlot //this tells the ghost what time it is so the time related conversations above will work.
	if SinceTalk >= 30 && AutoWeather == "ON" && weatherquiet == 2
	{
		if nowmode != "RYS"
		{
			weatherquiet = 1
			"\![raise,OnDownloadCurWeather]"
		}
	}
	--
 	if minute == 0 //These check what time it is. Here, it's seeing if the minute equals zero...
	{
		givenfood = 0 //resetting the cooldown on giving him food
		giveningredients = 0 //same but with ingredients -- He doesn't get one for warm things because you need to send him home to clear that one.
		givenwater = 0
		complimented = 0
		lastinteracted += 1
		RYSrecentBlankets = 0
		RYSrecentBooks = 0
		dreams = RAND(4)
		
		//if you've accidentally pet him in the wrong spot, it'll change some things about his petting dialogue until it ticks back down to 0
		if snagged > 0; {snagged -= 1}
		if flattered > 0; {flattered -= 2; if flattered < 0; {flattered = 0}}
		if comforted > 0; {comforted -= 2; if comforted < 0; {comforted = 0}}
		if talkedaboutpast > 0; {talkedaboutpast -= 2; if talkedaboutpast < 0; {talkedaboutpast = 0}}
		if recentpets > 0; {recentpets = 0}
		if hugged > 0; {hugged -= 1; if hugged < 0; {hugged = 0}} //hugged =/= totalhugs
		if changedscaling > 1; {changedscaling -= 1} //if it's above 1 you've changed it recently, but it never goes under 1. If it's at least 1, you've changed it at least once.
		if Dragged > 0; {Dragged = 0} //if it's above 1 you've changed it recently, but it never goes under 1. If it's at least 1, you've changed it at least once.
		if RYSrecentBlankets > 0; {RYSrecentBlankets -= 2; if RYSrecentBlankets < 0; {RYSrecentBlankets = 0}}
		if RYSrecentBooks > 0; {RYSrecentBooks -= 2; if RYSrecentBooks < 0; {RYSrecentBooks = 0}}
		if sleephour > 0; {sleephour -= 2; if sleephour < 0; {sleephour = 0}}
		if recentnightmare != 0; {recentnightmare = 0}
		--
		if autoupdate == "ON" && askedalready == 0 && updateavailable == 0 && AutoUpdateCheck == 0
		{
			AutoUpdateCheck = 1
			"\![updatebymyself,checkonly]"
		}
		--
		if hour == 0
		{
			if nowmode == "RYS"
			{
				"\RYS%(RYSIdle)"
			}
			else
			{
				"\0"
			}
			--
			"* (It's midnight.)\e"
			askedalready = 0 //resets him asking about updates
			AutoUpdateCheck = 0 //Resets him checking for new updates... I think I need this??? Idk man
		}
		elseif hour == 12
		{
			if nowmode == "RYS"
			{
				"\RYS%(RYSIdle)"
			}
			else
			{
				"\0"
			}
			--
			"* (It's noon.)\e"
		}
		else
		{
			if nowmode == "RYS"
			{
				"\RYS%(RYSIdle)"
			}
			else
			{
				"\0"
			}
			--
			"* (It's %(hour12)'o clock.)\e"
			//%(hour12) stores the time. This is a general top of the hour notification. I personally like this because it helps me keep track of time when I've been working on something, so when a ghost is all "Hey it's 3 o clock" I can be like woah really when did that happen. If you think this is annoying though, you can just delete this bit of dialogue to leave this else statement blank, or leave "" in here instead.
		}
	}
	elseif minute == 25 && LV == 0
	{
		if nowmode == "RYS"
		{
			
		}
		else
		{
			if hour == 8 || hour == 20 //it's the bad time. Doesn't work so well if you've got a 24 hour clock but w/e
			{
				"\0\s[200]* (He's staring very pointedly at the side of your screen.)\w8\w8\n\n* (Is he trying to avoid looking at your clock..?)\e"
				"\0\s[204]* (He's muttering something under his breath...)\w8\w8\n\n* (Something about digital clocks?)\e"
			}
		}
	}
	//Don't touch this part.
	/*elseif mikireflag == 1
 	{
 		MikireTalk
	}*/
}

OnOffscreen
{
	if reference0 == 0
	{
		mikireflag = 1
		MikireTalk
	}
	elseif reference1 == 0 && reference0 == 1
	{
		"Back"
	}
}

//--OnSecondChange
//Like the above, this handles what happens every time a second goes by. Most of this you will not have to touch.

OnSecondChange
{
	//SinceTalk = GETSECCOUNT() - SHIORI3FW.TalkEndTime //This gets the seconds since the last time he spoke. This is useful for a couple of things...
	if SHIORI3FW.CanTalk == 1 //Scratch that, the above gets the seconds since the last script STARTED. This little counter gets it since it ended.
	{
		SinceTalk++
	}
	else
	{
		SinceTalk = 0
	}
	--
	if passsec == 59
 	{
	 	passsec = 0
 		passmin++
	}
 	else
 	{
	 	passsec++
	}
	if nowmode == "Asleep"
	{
		if sleepsec == 59
		{
			sleepsec = 0
			sleepmin++
			if sleepmin >= 59
			{
				sleepmin = 0
				sleephour++
				totalsleep++
			}
		}
		else
		{
			sleepsec++
		}
	}
	--
	if mikireflag == 1 && reference1 == 0
	{
		EndMikireTalk
	}
	mikireflag = reference1
	if reference2 == 1
	{

	}
}
 
Hourcheck
{
	if passmin >= 59
	{
		passmin = 0
		passhour++
		if nowmode == "RYS"
		{
			RYShours++
		}
		else
		{
			totalhours++
		}
		if totalhours < 20 && lastinteracted > 6
		{
			if LV == 0
			{
				love += 0.5
			}
		}
		--
		if AutoWeather == "ON"
		{
			if nowmode != "RYS"
			{
				weatherquiet = 1
				"\![raise,OnDownloadCurWeather]"
			}
		}
		else
		{
			tempurature = "Unknown"
			ForC = "Unknown"
			feelslike = "Unknown"
			humidity = "Unknown"
			wind = "Unknown"
			weather = "Unknown"
			pollen = "Unknown"
			phase = "Unknown"
		}
	}
}

