OnCheckPosition //Checking his position on the screen. This'll be called somewhere in RandomTalk.
{
	XCoord = TOINT(reference0[0])
	YCoord = TOINT(reference0[1])
	XCoord = (XCoord + 115) //S's frames are 230 long, so this puts the coord in the middle of hit hitbox
	YCoord = (YCoord + 135) //Same here but with Y
	if XCoord < 0 {NewXMonitor = 2}
	elseif XCoord > displaywidth {NewXMonitor = 2}
	else {NewXMonitor = 1}
	--
	if YCoord > displayheight {NewYMonitor = 2}
	elseif YCoord < 0 {NewYMonitor = 2}
	elseif YCoord < (displayheight / 2) {NewYMonitor = 1}
	else {NewYMonitor = 1}
	--
	if Dragged > 6 //This has to be one higher than the value in mouse or wherever the dragging stuff is, otherwise this pops up after the balloon breaks
	{
		"\0\s[106]Kiddo,\w4 could you cut that out please?\w8\w8\s[107] I'm starting to get dizzy."
		"\0\s[107]Kiddo,\w4 can you stop moving me around so much?\w8\w8\s[106] It's pretty disorienting moving so quickly..."
		"\0\s[106]Kiddo,\w4 please,\w4 I'm getting dizzy.\w8\w8\s[107] Can you let me just stand here for a bit?"
	}
	elseif FromDragging != 2
	{
		if NewXMonitor != CurrentXMonitor || NewYMonitor != CurrentYMonitor //If you've dragged him onto a different monitor
		{
			if NewXMonitor == 1 && NewYMonitor == 1
			{
				"\0\s[0]Back over here,\w4 huh?\w8\w8\s[104] Ok.\w8\w8\s[101] What do you even use a second monitor for,\w4 anyways?"
				"\0\s[0]Is this your main monitor?\w8\w8\s[107] I didn't even realize people had more than one..."
			}
			elseif NewXMonitor == 2 || NewYMonitor == 2
			{
				if MultiMonitor == 0
				{
					MultiMonitor = 1
					"\0\s[110]You've got a whole other monitor?\w8\w8\s[107] I guess this must not be a laptop."
					//"\0\s[110]Another monitor?\w8\w8\s[106] Ok I guess."
					//"\0\s[110]A second monitor?\w8\w8\s[101] Do you need that much space?"
				}
				else
				{
					"\0\s[101]You must use your computer a lot if you've got two whole monitors,\w4 right?\w8\w8\s[107] Whatever floats your boat,\w4 I guess.\e"
					"\0\s[106]Am I that much in your way that you need to push me onto a whole other monitor?\e"
					"\0\s[107]Ah,\w4 back on the weird side of your desktop...\w8\w8\w8\s[202] I kid,\w4 I can't really tell much difference.\e"
				}
			}
		}
		else //If you've dragged him around but he's on the same monitor
		{
			if CurrentYPos > (displayheight / 2) && YCoord < (displayheight / 2) //Up high
			{
				"\0\s[110]Whoa.\w8\w8\s[106] Uh,\w4 I guess I'm up here now?\w8\w8\s[208] This is kinda weird..."
				"\0\s[110]Whoa,\w4 uh,\w8\s[106] is there a reason I'm up here?"
				"\0\s[110]Uh,\w8\s[106] am I in your way?\w8\w8\s[107] I'm not afraid of heights or anything,\w4 but I'd rather be on the ground..."
			}
			elseif CurrentYPos < (displayheight / 2) && YCoord > (displayheight / 2) //Back down low
			{
				"\0\s[107]Whew.\w8\w8\s[106] I guess it's kinda neat that I can float while I'm here,\w4 but I'd much rather stay on the ground."
				"\0\s[107]Whew,\w8\s[106] I'm glad to be on the ground again."
				"\0\s[106]Whew.\w8\w8\s[107] Thanks,\w4 %(username)."
			}
			elseif (XCoord - CurrentXPos) < -(displaywidth / 5) || (XCoord - CurrentXPos) > (displaywidth / 5) //Side to side
			{
				"\0\s[110]Oh,\w4 am I in your way %(username)?\w8\w8\s[106] Sorry about that."
				"\0\s[106]I hope I'm not getting in your way,\w4 %(username)."
			}
		}
	}
	--
	FromDragging = 0
	CurrentXMonitor = NewXMonitor
	CurrentYMonitor = NewYMonitor
	CurrentXPos = XCoord
	CurrentYPos = YCoord
	"\e"
}

