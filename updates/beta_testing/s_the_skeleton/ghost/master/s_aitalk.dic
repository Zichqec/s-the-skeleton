					if ghostexcount > 0 //I wanted to just make this call the communicate function but then I remembered you can't do that without him calling it EVERY time.... sigh //Idk what I'm talking about here, pretty sure I can do this with raise. Might've written that before I figured out how to use raise.
					{
						_number = RAND(ghostexcount) // This checks the number of other ghosts summoned and chooses a random number based on it
						--
						checkname = "%(ghostexlist[_number])" // This grabs the reference name of the ghost to check against known names.
						--
						// This will tell you the reference names of your summoned ghosts to use in the "when" cases below, along with the number of ghosts.
						// Comment it out when you no longer need it for reference.
						//"\0\s[0]Ghost List: %(ghostexlist)\n\n[half]Noticed %(checkname), %(_number) of %(ghostexcount)\x"
						--
						// Checking the selected loaded ghost's name vs known names to comment on them
						case checkname
						{
							when "Gaster"
							{
								"\0\s[200]* (He's avoiding looking at Gaster...)\e"
								"\0\s[200]* (He seems a little put off by Gaster's presence...)\e"
								"\0\s[200]* (He seems a little agitated,\w4 maybe because Gaster is here...)\e"
							}
							when "Sanguine" //S wasn't my first attempt to make a ghost...
							{
								"\0\s[106]Is uh...\w8\w8\s[202] Is that me..?\e"
							}
							when "RYS"
							{
								"\0\s[206]Ah...\w8\w8 That's...\e"
							}
							when "Temmie"
							{
								"\0\s[104]Heh,\w4 I should've known Temmie would be in a place like this.\e"
							}
							when "Hydrate"
							{
								"\0\s[110]Is...\w8\w8 is that a giant water bottle?\e"
							}
							others
							{
								"\![raise,OnStartTalking]"
							}
						}
					}
					
					if ghostexcount > 4 //Or, if there's more than 4...
					{
						"\0\s[106]It's uh...\w8\w8\s[202] a little crowded here,\w4 isn't it?\e"
						"\0\s[208]There are sure a lot of people here...\e"
						"\0\s[106]Your desktop is a popular place,\w4 huh?\e"
					}
					if ghostexcount == 0
					{
						"\0\s[206]It's so quiet here...\w8\w8\s[106] It's a little lonely,\w4 isn't it?\e"
					}
				}
				//====================================================Doesn't trust yet========================================================
				else //He isn't sure what to make of you yet
				{
					//"\0\s[206]It's so quiet here...\e" //oops I forgot about music players and youtube. Hrm. It's a shame these things cant detect whether the desktop is silent or not...
					"\0\s[206]* (He glances around your desktop,\w4 taking note of the various features.)\e"
					if AutoWeater != "ON"
					{
						"\0\s[106]So uh...\w8\w8 What's the weather like out there?\w8\w8\s[206]\n\nIt doesn't seem like there's weather at all in here...\e"
					}
					"\0\s[206]* (He doesn't say anything...)\w8\w8\n\n* (Maybe you should try interacting with him?)\e"
					"\0\s[200]* (He seems a little tense.)\e"
					if snagged > 0 //any time that you've snagged his head and you're not on a negative route
					{
						"\0\s[200]* (He sighs and rubs his head a little.)\w8\w8\n\n* (You wonder if it still hurts from when you snagged it...)\e"
					}
					if LVCheck == 1 && broken == 0 //runs at any time that you know his secrets but he's not broken
					{
						"\0\s[204]* (He's taking deep,\w2 slow breaths.)\e"
						"\0\s[200]* (He looks wistful.)\w8\w8\w8\n\n\s[204]* (You wonder if he's thinking about the past again...)\e"
					}
				}
				
				
			}
		}
		
		elseif LV > 0 //Negative routes
		{
			if LVCheck == 1 //know his secrets
			{

					if broken == 1 //broken him down all the way
					{
						"\s[510]* (He doesn't say anything,\w4 just stands there silently,\w4 awaiting your judgement.)\e"
						"\s[510]..."
						"\s[510]..."
						"\s[510]..."
					}
					else //breaking him, but he's still standing
					{
						"\s[500]* (He shivers quietly.)\e"
						"\s[516]* (He shifts on his feet nervously.)\e"
						"\s[500]* (You can see his sins crawling on his back.)\e"
					}
				
			}
			else //Just mean for no reason
			{
				if LV <= 30 //you've been a bit mean
				{
					"\0\s[404]* (He looks around your desktop,\w4 and you wonder if he's searching for an exit.)\e"
					"\0\s[402]Is there a reason you're keeping me here?\w8\w8 Am I just a glorified punching bag to you or something?\e"
					"\0\s[402]You know,\w4 if you hate me so much maybe you shouldn't have brought me here.\w8\w8\s[401]\n\nI guess the concept of live and let live is lost on you.\e"
				}
				elseif LV >=31 //You've been really mean and he hates you a lot
				{
					"\0\s[404]* (He looks around your desktop,\w4 and you wonder if he's searching for an exit.)\w8\w8\n\n* (There isn't one.)\e"
					"\0\s[414]* (Seems like he doesn't want to talk to you.)\e"
					"\0\s[400]* (He's ignoring you.)\e"
					"\0\s[400]* (He doesn't say a word.)\e"
				}
			}
		}
	}
}

