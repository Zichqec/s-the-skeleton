	}
}

OnWindowStateMinimize
{
	if SomeoneHoldingObsidian != ""
	{
		res_reference0 = SomeoneHoldingObsidian[0]
		_ghost = SomeoneHoldingObsidian[0]
		_num = SomeoneHoldingObsidian[1]
		SomeoneHoldingObsidian = ""
		
		"\e DustyMinimized%(_ghost) %(_num)"
	}
}


//--OnGhostChanging
 
OnGhostChanging
 {
