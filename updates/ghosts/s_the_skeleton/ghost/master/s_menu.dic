MenuRun : all
{
	lastinteracted = 0
	if nowmode == "RYS"; {"\![raise,OnRYSMenu]"; return}
	
	"\_q\b2\![set,autoscroll,disable]\![no-autopause]\f[anchorvisitedfontcolor,default.anchor]"
	"\n\n"
	"* (You ask S...)"
	"\n\n[half]"
	
	if lastTalk != "" //Has said at least 1 dialogue this session
	{
		if LV > 0; "\![*]\q[To say that again,OnLastTalk]"
		else; "\![*]\q[To repeat himself,OnLastTalk]"
		"\n"
	}
	"\![*]To talk \_a[OnAiTalk,userprompted]to you\_a or to \_a[OnStartTalking]another ghost\_a\n"
	"\![*]\q[Some questions,ihavequestions]"
	"\n\n"
	
	"* (Or maybe instead you want to...)"
	"\n\n[half]"
	"\![*]\q[Do something,OnInteractMenu]\n"
	"\![*]\q[Use some functions,functionmenu]\n"
	"\![*]\q[Change some settings,configmenu]"
	if RYSprogress == 7; " or \![*]\__q[OnModeMenu]Change to...\__q"
	"\n\n"
	
	"\![*]\q[Nevermind,CANCEL]"
	if debug; "  \![*]\q[Debug,OnDebugMenu]  \![*]\__q[OnTestVariable]Test Variable\__q"
	"\n\n[half]"
	
	"\f[height,-1]%(wd) %(displayhour):%(nowmin) %(ap), %(month)/%(day)/%(year)  -  %(season)"
	if AutoWeather == "ON"
	{
		_temp = "??"; if temperature != ""; _temp = temperature
		_unit = "F"; if weather.units == "Metric"; _unit = "C"
		", %(temperature)°%(_unit) \_n%(moonphase)\_n"
	}
	"\nRuntime: %(passhour):%(displaymin)"
	"%(newline)\f[align,right]Version %(ghostver)"
	
	if CHAIN.IDName != ""; "\e:chain=end"
}

