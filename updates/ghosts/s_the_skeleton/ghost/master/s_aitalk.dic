OnCheckPosition //Checking his position on the screen. This'll be called somewhere in RandomTalk.
{
	XCoord = TOINT(reference0[0])
	YCoord = TOINT(reference0[1])
	XCoord = (XCoord + 115) //S's frames are 230 long, so this puts the coord in the middle of hit hitbox
	YCoord = (YCoord + 135) //Same here but with Y
	if XCoord < 0
	{
		NewXMonitor = 2
	}
	elseif XCoord > displaywidth
	{
		NewXMonitor = 2
	}
	else
	{
		NewXMonitor = 1
	}
	--
	if YCoord > displayheight
	{
		NewYMonitor = 2
	}
	elseif YCoord < 0
	{
		NewYMonitor = 2
	}
	elseif YCoord < (displayheight / 2)
	{
		NewYMonitor = 1
	}
	else
	{
		NewYMonitor = 1
	}
	--
	if Dragged > 5
	{
		"\0\s[106]Kiddo,\w4 could you cut that out please?\w8\w8\s[107] I'm starting to get dizzy."
		"\0\s[107]Kiddo,\w4 can you stop moving me around so much?\w8\w8\s[106] It's pretty disorienting moving so quickly..."
	}
	elseif FromDragging != 2
	{
		if NewXMonitor != CurrentXMonitor || NewYMonitor != CurrentYMonitor //If you've dragged him onto a different monitor
		{
			if NewXMonitor == 1 && NewYMonitor == 1
			{
				"\0\s[0]Back over here,\w4 huh?\w8\w8\s[104] Ok.\w8\w8\s[101] What do you even use a second monitor for,\w4 anyways?"
			}
			elseif NewXMonitor == 2 || NewYMonitor == 2
			{
				"\0\s[110]You've got a whole other monitor?\w8\w8\s[107] I guess this must not be a laptop."
			}
		}
		else //If you've dragged him around but he's on the same monitor
		{
			if CurrentYPos > (displayheight / 2) && YCoord < (displayheight / 2)
			{
				"\0\s[110]Whoa.\w8\w8\s[106] Uh,\w4 I guess I'm up here now?\w8\w8\s[208] This is kinda weird..."
			}
			elseif CurrentYPos < (displayheight / 2) && YCoord > (displayheight / 2)
			{
				"\0\s[107]Whew.\w8\w8\s[106] I guess it's kinda neat that I can float while I'm here,\w4 but I'd much rather stay on the ground."
			}
			elseif (XCoord - CurrentXPos) < -(displaywidth / 5) || (XCoord - CurrentXPos) > (displaywidth / 5)
			{
				"\0\s[110]Oh,\w4 am I in your way %(username)?\w8\w8\s[106] Sorry about that."
				"\0\s[106]I hope I'm not getting in your way,\w4 %(username)."
			}
		}
	}
	--
	FromDragging = 0
	CurrentXMonitor = NewXMonitor
	CurrentYMonitor = NewYMonitor
	CurrentXPos = XCoord
	CurrentYPos = YCoord
	"\e"
}

