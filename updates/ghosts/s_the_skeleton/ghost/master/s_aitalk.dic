		if snagged > 0 //if you've accidentally pet him in the wrong spot, it'll change some things about his petting dialogue until it ticks back down to 0
		{
		snagged -= 1
		}
		--
		if flattered > 0
		{
			flattered -= 2
			if flattered < 0
			{
				flattered = 0
			}
		}
		--
		if comforted > 0
		{
			comforted -= 2
			if comforted < 0
			{
				comforted = 0
			}
		}
		--
		if talkedaboutpast > 0
		{
			talkedaboutpast -= 2
			if talkedaboutpast < 0
			{
				talkedaboutpast = 0
			}
		}
		--
		if recentpets > 0
		{
			recentpets = 0 
		}
		--
		if hugged > 0 //hugged =/= totalhugs
		{
			hugged -= 1
		}
		--
		if changedscaling > 1 //if it's above 1 you've changed it recently, but it never goes under 1. If it's at least 1, you've changed it at least once.
		{
			changedscaling -= 1
		}
		--
		if Dragged > 0 //if it's above 1 you've changed it recently, but it never goes under 1. If it's at least 1, you've changed it at least once.
		{
			Dragged = 0
		}
		--
		if hour == 0
		{
			"\0\s[0]* (It's midnight.)\e"
			--
			if autoupdate == "ON"
			{
				"\![updatebymyself,checkonly]"
			}
		}
		elseif hour == 12
		{
			"\0\s[0]* (It's noon.)\e"
		}
		else
		{
			"\0\s[0]* (It's %(hour12)'o clock.)\e"
			//%(hour12) stores the time. This is a general top of the hour notification. I personally like this because it helps me keep track of time when I've been working on something, so when a ghost is all "Hey it's 3 o clock" I can be like woah really when did that happen. If you think this is annoying though, you can just delete this bit of dialogue to leave this else statement blank, or leave "" in here instead.
		}
	}
	elseif minute == 25 && LV == 0
	{
		if hour == 8 //it's the bad time
		{
			"\0\s[200]* (He's staring very pointedly at the side of your screen.)\w8\w8\n\n* (Is he trying to avoid looking at your clock..?)\e"
		}
		elseif hour == 20 //it's the other bad time, unless you're using a 24 hour clock. This doesn't really work if you're using 24 hour. Eh.
		{
			"\0\s[200]* (He's staring very pointedly at the side of your screen.)\w8\w8\n\n* (Is he trying to avoid looking at your clock..?)\e"
		}
	}
	//Don't touch this part.
	elseif mikireflag == 1
 	{
 		MikireTalk
	}
}

//--OnSecondChange
