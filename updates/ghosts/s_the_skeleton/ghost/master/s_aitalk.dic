//---------------------------AI Talk--------------------------
AiTalk
{
	if mikireflag == 0
	{
		if CHAIN.IDName == ""
		{
			lastTalk = RandomTalk
		}
		else
		{
			lastTalk = ChainTalk
		}
		lastTalk
	}
	else
	{ 
		MikireTalk
	}
}

RandomTalk
{
	if nowmode == "RYS"
	{
		OnRYSRandomTalk
	}
	else
	{
		if LV == 0 //Friendship routes
		{
			_sleepcheck = RAND(100)
			if lookingatphone == 1; PhoneTalk
			elseif holdingObsidian == 1; HoldingObsidianTalk
			elseif currentlydrinking > 0
			{
				drinkcounter--
				if drinkcounter <= 0; FinishDrinking
				elseif currentlydrinking == 1; DrinkingWaterTalk
				elseif currentlydrinking == 2; DrinkingJuiceTalk
				elseif currentlydrinking == 3; DrinkingMilkTalk
			}
			elseif nowmode == "Asleep"; AsleepTalk
			elseif _sleepcheck <= TOINT(sleepchance) && love >= 150 && reference0 != "userprompted"; FallAsleepTalk
			elseif lastinteracted >= 20; GoneSuperLongTalk
			elseif lastinteracted >= 6; GoneLongTalk
			else
			{
				if love >= 75
				{
					if !Flag("Said daily dialogue") && (RAND(100) >= 95); OnDailyDialogues
					else; MainTalk
				}
				elseif love >= 20; LowTrustTalk
				else; DontTrustTalk
			}
		}
		else //Negative routes
		{
			if LVCheck == 1 //Sad
			{
				if broken == 1; BrokenTalk
				else; SadTalk
			}
			else //Mad
			{
				if LV <= 30; MadTalk
				else; HateTalk
			}
		}
	}
}

