			"\0* (He disappears\s[-1] into your files...)\n\n\w8\w8* (Perhaps he's looking for something to keep himself occupied.)\e"
		}
	}
	
	elseif LV > 0 //Negative routes
	{
		if LVCheck == 1 //know his secrets
		{

				if broken == 1 //broken him down all the way
				{
					"\s[510]* (He doesn't say anything, just stands there silently, awaiting your judgement.)\e"
					"\s[510]..."
					"\s[510]..."
					"\s[510]..."
				}
				else //breaking him, but he's still standing
				{
					"\s[500]* (He shivers quietly.)\e"
					"\s[516]* (He shifts on his feet nervously.)\e"
					"\s[500]* (You can see his sins crawling on his back.)\e"
				}
			
		}
		else //Just mean for no reason
		{
			if LV <= 30 //you've been a bit mean
			{
				"\0\s[404]* (He looks around your desktop, and you wonder if he's searching for an exit.)\e"
				"\0\s[402]Is there a reason you're keeping me here?\w8\w8 Am I just a glorified punching bag to you or something?\e"
				"\0\s[402]You know, if you hate me so much maybe you shouldn't have brought me here.\w8\w8\s[401]\n\nI guess the concept of live and let live is lost on you.\e"
			}
			elseif LV >=31 //You've been really mean and he hates you a lot
			{
				"\0\s[404]* (He looks around your desktop, and you wonder if he's searching for an exit.)\w8\w8\n\n* (There isn't one.)\e"
				"\0\s[414]* (Seems like he doesn't want to talk to you.)\e"
				"\0\s[400]* (He's ignoring you.)\e"
				"\0\s[400]* (He doesn't say a word.)\e"
			}
		}
	}
}

