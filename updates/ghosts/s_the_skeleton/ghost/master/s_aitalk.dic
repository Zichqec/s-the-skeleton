	if SHIORI3FW.CanTalk == 1 //Scratch that, the above gets the seconds since the last script STARTED. This little counter gets it since it ended.
	{
		SinceTalk += 1
	}
	else
	{
		SinceTalk = 0
	}
	--
	if passsec == 59
 	{
	 	passsec = 0
 		passmin = passmin + 1
	}
 	else
 	{
	 	passsec = passsec + 1
	}
	if mikireflag == 1 && reference1 == 0
	{
		EndMikireTalk
	}
	mikireflag = reference1
	if reference2 == 1
	{

	}
 }
 
Hourcheck
 {
	if passmin >= 59
	{
		passmin = 0
		passhour += 1
		totalhours += 1
		if totalhours < 20 && lastinteracted > 6
		{
			if LV == 0
			{
				love += 0.5
			}
		}
		--
		if AutoWeather == "ON"
		{
			weatherquiet = 1
			"\![raise,OnDownloadCurWeather]"
		}
	}
	--
	if passmin <= 9
	{
		displaymin = "0" + passmin //[Zi]maaaaybe?? -- Yes ok this works, this makes it so the minutes on the runtime have a 0 if they're under 9, so they format correctly. This is the kind of stupid detail that bothers me a lot.
	}
	elseif passmin >=10
	{
		displaymin = passmin
	}
 }




 
