DreamPools : nonoverlap //Separate function for nonoverlap/checking amounts with array
{
	parallel Pool("Pool_Sleep_BestDreams","%(dreams == 4)")
	parallel Pool("Pool_Sleep_GoodDreams","%(dreams >= 3)")
	parallel Pool("Pool_Sleep_NeutralDreams","%(dreams >= 1 && dreams <= 3)")
	parallel Pool("Pool_Sleep_BadDreams","%(dreams <= 1)")
	parallel Pool("Pool_Sleep_WorstDreams","%(dreams == 0)")
}
