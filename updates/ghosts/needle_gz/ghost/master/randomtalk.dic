OnNeedleWarp
{
	_rate = 3
	_speed = RAND(100) + 10
	_targetX = RAND(600) - 300
	_targetY = RAND(600) - 300
	
	if reference1 == ""
	{
		reference1 = reference0[1]
		reference0 = reference0[0]
	}
	
	_nowX = 100
	if reference2 != ""; _nowX = reference2
	_nowY = 100
	if reference3 != ""; _nowY = reference3
	
	_cmd = ""
	
	while !(_nowX == _targetX && _nowY == _targetY)
	{
		if _nowX > _targetX
		{
			if _nowX - _targetX <= _rate; _nowX = _targetX
			else; _nowX -= _rate
		}
		else //Less than target X
		{
			if _targetX - _nowX <= _rate; _nowX = _targetX
			else; _nowX += _rate
		}
		if _nowY > _targetY
		{
			if _nowY - _targetY <= _rate; _nowY = _targetY
			else; _nowY -= _rate
		}
		else //Less than target Y
		{
			if _targetY - _nowY <= _rate; _nowY = _targetY
			else; _nowY += _rate
		}
		_cmd += "\![set,scaling,%(_nowX),%(_nowY)]\_w[%(_speed)]"
	}
	
	"\t"
	--
	_cmd
	--
	ErrorLog("ref0: %(reference0); ref1: %(reference1); _nowX: %(_nowX); _nowY: %(_nowY); reference2: %(reference2); reference3; %(reference3)","info")
	ErrorLog("![raise,OnNeedleWarp,%(reference0),%(reference1),%(_nowX),%(_nowY)]","info")
	"\![raise,OnNeedleWarp,%(reference0),%(reference1),%(_nowX),%(_nowY)]"
	{
		"\w8\![set,scaling,100,100]"
		--
		"\![move,--X=%(reference0),--Y=%(reference1),--base=primaryscreen]"
	}
}

OnNeedleSpin : all
{
	_amt = _argv[0]
	if _amt < 1; _amt = 1
	_spd = _argv[1]
	if _spd < 1; _spd = 1
	_rate = _argv[2]
	if _rate < 1; _rate = 1
	_debug = 0
	
	"\![set,scaling,100,100]"
	for _i = 0; _i < _amt; _i++
	{
		_nowX = 100
		
		while _nowX >= -100
		{
			_debug++
			_nowX -= _rate
			"\![set,scaling,%(_nowX),100]"
			if _nowX >= 0; "\![move,--X=%(_rate),--base=me]"
			else; "\![move,--X=-%(_rate),--base=me]"
			"\_w[%(_spd)]"
		}
		//"\![move,--X=%(_rate),--base=me]"
		while _nowX < 100
		{
			_debug--
			_nowX += _rate
			"\![set,scaling,%(_nowX),100]"
			if _nowX >= 0; "\![move,--X=-%(_rate),--base=me]"
			else; "\![move,--X=%(_rate),--base=me]"
			"\_w[%(_spd)]"
		}
	}
	ErrorLog(_debug,"info")
	"\![set,scaling,100,100]"//"\![move,--X=-%(_rate),--base=me]"
}

