screamrand : all
{
	_rand = RAND(20) + 10
	if _argv[0] > 0; _rand = RAND(_argv[0]) + 10
	
	for _i = 0; _i < _rand; _i++
	{
		random:/
		{
			"A"
			"E"
			"I"
		}
	}
}

