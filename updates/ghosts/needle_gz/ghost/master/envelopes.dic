poked
{
	_poke = poke
	if _argv[0] != ""; _poke = _argv[0]
	_upper = 0
	
	if ISUPPER(_poke); _upper = 1
	
	_poke = TOLOWER(_poke)
	
	if EndsWith(_poke,"e")
	{
		_poke += "d"
	}
	else
	{
		_special = ("stab","prod","jab","stir","dig","nag","ram")
		_one = SPLIT(_poke," ")[0]
		if ASEARCH(_one,_special) != -1
		{
			_len = STRLEN(_poke)
			_poke += SUBSTR(_poke,_len - 1,1) + "ed"
		}
		else; _poke += "ed"
	}
	
	if _upper; TOUPPER(_poke)
	else;_poke
}

