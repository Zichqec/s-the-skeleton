FadeIn : all
{
	_amt = 5
	if _argv[0] > 0; _amt = _argv[0]
	
	for _i = 0; _i < 100; _i += _amt
	{
		"\![set,alpha,%(_i)]\w1"
	}
	"\![set,alpha,100]"
}

FadeOut : all
{
	_amt = 5
	if _argv[0] > 0; _amt = _argv[0]
	
	for _i = 100; _i >= 0; _i -= _amt
	{
		"\![set,alpha,%(_i)]\w1"
	}
	"\![set,alpha,0]"
}


