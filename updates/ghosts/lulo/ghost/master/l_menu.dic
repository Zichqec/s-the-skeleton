	SETDELIM(_playlists,"|")
	_file = _playlists[reference0]
	
	_f = FOPEN("playlists/%(_file)","r")
	--
	for _buff = FREAD("playlists/%(_file)"); _buff != -1; _buff = FREAD("playlists/%(_file)")
	{
