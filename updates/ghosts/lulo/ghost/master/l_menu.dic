StartRecursive
{
	Recursive = 0
	RecursiveTest('One','Two','Three')
	ERASEVAR("Recursive")
}

RecursiveTest
{
	_array = IARRAY
	_recursive = IARRAY
	foreach _argv; _arg
	{
		Recursive++
		_array ,= _arg
	}
	if Recursive < 10
	{
		LOGGING("===START RECURSIVE FUNCTION===")
		_recursive = RecursiveTest(_argv)
		LOGGING("===RECURSION DONE, GOT %(_recursive)===")
		_array ,= _recursive
	}
	LOGGING("Returning: %(_array)")
	_array
}

