	_spacer = FREAD("journal/%(reference0)")
	
	_buff = ""
	_display = ""
	for _i = 0; _buff != -1; _i++
	{
		_buff = FREAD("journal/%(reference0)")
		if _buff == -1; break //If we run out of file, break the loop so we don't add extra tags
		
		_display += "\_?" + _buff + "\_?"
